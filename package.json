{
    "name": "@mirei/mona-dock-layout",
    "version": "1.0.0",
    "description": "A dock layout component for Angular",
    "main": "dist/mona-dock-layout/fesm2022/mona-dock-layout.mjs",
    "exports": {
        ".": "./dist/mona-dock-layout/fesm2022/mona-dock-layout.mjs"
    },
    "typings": "./dist/mona-dock-layout/index.d.ts",
    "scripts": {
        "ng": "ng",
