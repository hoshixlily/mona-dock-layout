        "build:watch": "ng build mona-dock-layout --watch --configuration development",
        "watch": "ng build mona-dock-layout --watch --configuration development",
        "test": "ng test"
    },
    "author": "Lumine",
    "files": [
        "dist",
        "README.md"
    ],
